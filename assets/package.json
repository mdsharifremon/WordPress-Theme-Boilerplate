{
  "name": "wordpress-theme-boilerplate",
  "version": "1.0.0",
  "description": "A basic wordpress theme boilerplate",
  "main": "index.js",
  "keywords": [
    "wordpress",
    "theme",
    "boilerplate"
  ],
  "author": "Sharif Uddin",
  "license": "MIT",
  "private": true,
  "browserslist": [
